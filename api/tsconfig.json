{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["./src/*"],
      "@prismaG/*": ["./src/generated/prisma/*"],
      "@interfaces/*": ["./src/interfaces/*"],
      "@middlewares/*": ["./src/middlewares/*"],
      "@enums/*": ["./src/enums/*"],
      "@routes/*": ["./src/routes/*"],
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}